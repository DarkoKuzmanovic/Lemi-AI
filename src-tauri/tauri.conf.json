{
  "build": {
    "devPath": "../web",
    "distDir": "../web",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "lemi-ai",
    "version": "0.2.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 900,
        "resizable": true,
        "title": "Lemi AI",
        "width": 1400,
        "url": "https://chatgpt.com/g/g-W0eMwXPRv-lemi-ai"
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.lemi-ai.app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
